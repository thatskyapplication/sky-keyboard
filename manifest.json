	"manifest_version": 3,
	"name": "Sky Keyboard",
	"version": "1.0.0",
	"description": "Enhancements for the Sky Keyboard.",
	"permissions": ["activeTab"],
	"content_scripts": [
		{
			"matches": ["https://keyboard.sky.thatgame.co/*"],
			"js": ["distribution/index.js"],
			"run_at": "document_end"
		}
	]
}
